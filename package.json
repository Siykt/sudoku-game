{
  "name": "sudoku-game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 10001 --mode development",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "prepare": "husky install",
    "lint": "cross-env DEBUG=eslint:cli-engine eslint --cache --ext=ts,tsx --fix src",
    "format": "prettier --write \"src/**/*.@(ts|tsx|)\""
  },
  "dependencies": {
    "@saitonakamura/react-pixi": "^7.0.0-beta-5",
    "cross-env": "^7.0.3",
    "pixi.js": "^6.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
